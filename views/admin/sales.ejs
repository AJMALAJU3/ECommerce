            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header" style="border-bottom-color: #c96;">
                            <h4>Sales Report</h4>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">

                                <table class="table table-striped table-hover" id="tableExport" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Date</th>
                                            <th style="width: 10em;">Products</th>
                                            <th>Payment</th>
                                            <th style="width: 15em;">Discount</th>
                                            <th style="width: 15em;">Price</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% orders.forEach(order=> { %>
                                            <tr>
                                                <% let totalOldPrice=0; order.product.forEach(item=> { if
                                                    (item.productDetails.status == 'delivered') { totalOldPrice +=
                                                    item.productDetails.productPrices.priceBefore * item.quantity; } });
                                                    %>
                                                    <td>
                                                        order id : <%= order.userId %> <br>
                                                            user id : <%= order.userId %>
                                                    </td>
                                                    <% const dateObject=new Date(order.createdAt); const options={
                                                        weekday: 'short' , year: 'numeric' , month: 'short' ,
                                                        day: '2-digit' }; const
                                                        formattedDate=dateObject.toLocaleDateString('en-US', options);
                                                        %>
                                                        <td>
                                                            <%= formattedDate %>
                                                        </td>

                                                        <td class="text-truncat">

                                                            <ul class="list-unstyled order-list m-b-0 m-b-0 d-flex">
                                                                <% order.product.forEach(product=> { if
                                                                    (product.productDetails.status != 'cancelled') { %>
                                                                    <li class="team-member team-member-sm">
                                                                        <a
                                                                            href="/admin/productDetails?id=<%= product.productDetails._id %>">
                                                                            <img class="rounded-circle"
                                                                                src="<%= product.productDetails.productImage[0] %>"
                                                                                alt="user" data-toggle="tooltip"
                                                                                title=""
                                                                                data-original-title="<%= product.productDetails.productTags[0] %>">
                                                                        </a>
                                                                    </li>
                                                                    <% } }) %>
                                                                        <div class="badge text-success"
                                                                            style="margin-left: auto;">
                                                                            <%= order.status %>
                                                                        </div>
                                                            </ul>


                                                        </td>


                                                        <td>
                                                            payment method : <%= order.payment.paymentMethod %> <br>
                                                                shipping charge :<%= order.shippingCharge.shippingCharge
                                                                    %>
                                                        </td>

                                                        <td>
                                                            product discount :<%= (100 -
                                                                order.totalPrice/(totalOldPrice/100)).toFixed(1) %>%
                                                                <br>
                                                                coupon discount :<%= order.appliedCoupon.couponDiscount
                                                                    %>rs <br>
                                                                    <h5>total discount :<%= parseFloat((100 -
                                                                            (order.grandTotalPrice -
                                                                            (order.shippingCharge.shippingCharge)) /
                                                                            (totalOldPrice / 100)).toFixed(1)) %>%
                                                                    </h5>
                                                        </td>



                                                        <td>
                                                            total price :<%= totalOldPrice %> <br>
                                                                offerred price :<%= order.totalPrice %> <br>
                                                                    <h5>grand total price :<%= order.grandTotalPrice %>
                                                                    </h5>

                                                        </td>


                                            </tr>
                                            <% }) %>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>




